generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String  @id @default(cuid())
  
  email String @unique
  password String
  name String?

  createdAt DateTime @default(now()) @map("created_at")
}
